        <omgdi:waypoint x="428.0" y="358.0"></omgdi:waypoint>
        <omgdi:waypoint x="428.0" y="407.0"></omgdi:waypoint>
        <omgdi:waypoint x="560.0" y="407.0"></omgdi:waypoint>
